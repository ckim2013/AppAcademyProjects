<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <h1>
      MUSIC APP
    </h1>

    <% if current_user %>
      <%= button_to "SIGN OUT", session_url, method: :delete, class: "sign" %>
    <% end %>

    <%= yield %>
  </body>
</html>
